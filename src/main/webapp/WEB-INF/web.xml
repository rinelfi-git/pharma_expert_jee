		<servlet-class>mg.adequa.servlets.SLogin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JournalDeSession</servlet-name>
		<servlet-class>mg.adequa.servlets.SJournalDeSession</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Notification</servlet-name>
		<servlet-class>mg.adequa.servlets.SNotification</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Autorisation</servlet-name>
		<servlet-class>mg.adequa.servlets.SAutorisation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AutreEntree</servlet-name>
		<servlet-class>mg.adequa.servlets.SAutreEntree</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AutreSortie</servlet-name>
		<servlet-class>mg.adequa.servlets.SAutreSortie</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BonDeCommande</servlet-name>
		<servlet-class>mg.adequa.servlets.SBonDeCommande</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ContratUrgence</servlet-name>
		<servlet-class>mg.adequa.servlets.SContratUrgence</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Facture</servlet-name>
		<servlet-class>mg.adequa.servlets.SFacture</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Fournisseur</servlet-name>
		<servlet-class>mg.adequa.servlets.SFournisseur</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Menu</servlet-name>
		<servlet-class>mg.adequa.servlets.SMenu</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ServerDate</servlet-name>
		<servlet-class>mg.adequa.servlets.SServerDate</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Ong</servlet-name>
		<servlet-class>mg.adequa.servlets.SOng</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PersonneOng</servlet-name>
		<servlet-class>mg.adequa.servlets.SPersonneOng</servlet-class>
	</servlet>
	
	
	<servlet-mapping>
		<servlet-name>Achat</servlet-name>
		<url-pattern>/achat</url-pattern>
		<url-pattern>/achat/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Docteur</servlet-name>
		<url-pattern>/docteur</url-pattern>
		<url-pattern>/docteur/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>TableauDeBord</servlet-name>
		<url-pattern>/tableau-de-bord</url-pattern>
		<url-pattern>/tableau-de-bord/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/login</url-pattern>
		<url-pattern>/login/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>JournalDeSession</servlet-name>
		<url-pattern>/journal_de_session</url-pattern>
		<url-pattern>/journal_de_session/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Notification</servlet-name>
		<url-pattern>/notification</url-pattern>
