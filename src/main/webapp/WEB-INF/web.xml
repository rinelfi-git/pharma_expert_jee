		<url-pattern>/autre_sortie</url-pattern>
		<url-pattern>/autre_sortie/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>BonDeCommande</servlet-name>
		<url-pattern>/bon_de_commande</url-pattern>
		<url-pattern>/bon_de_commande/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ContratUrgence</servlet-name>
		<url-pattern>/contrat_urgence</url-pattern>
		<url-pattern>/contrat_urgence/*</url-pattern>
	</servlet-mapping>
	
