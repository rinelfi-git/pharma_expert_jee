		<servlet-name>Facture</servlet-name>
		<url-pattern>/facture</url-pattern>
		<url-pattern>/facture/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Fournisseur</servlet-name>
		<url-pattern>/fournisseur</url-pattern>
		<url-pattern>/fournisseur/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Menu</servlet-name>
		<url-pattern>/menu</url-pattern>
		<url-pattern>/menu/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>ServerDate</servlet-name>
		<url-pattern>/server_date/datetime</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>Ong</servlet-name>
		<url-pattern>/ong</url-pattern>
		<url-pattern>/ong/*</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>PersonneOng</servlet-name>
		<url-pattern>/personne_ong</url-pattern>
		<url-pattern>/personne_ong/*</url-pattern>
	</servlet-mapping>
</web-app>